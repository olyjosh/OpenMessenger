{
  "manifest_version": 2,
  "name": "Add whatsapp message option to number",
  "version": "0.1",
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Add WhatsApp message option to number",
    "default_popup": "newtab.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [ "content.js"]
    }
  ]
}
